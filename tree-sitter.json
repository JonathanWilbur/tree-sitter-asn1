{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "asn1",
      "camelcase": "Asn1",
      "title": "ASN.1",
      "scope": "source.asn1",
      "file-types": [
        "asn1",
        "asn"
      ],
      "injection-regex": "^asn1$",
      "class-name": "TreeSitterAsn1"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Abstract Syntax Notation",
    "authors": [
      {
        "name": "Jonathan M. Wilbur",
        "email": "jonathan@wilbur.space",
        "url": "https://github.com/JonathanWilbur"
      }
    ],
    "links": {
      "repository": "https://github.com/JonathanWilbur/tree-sitter-asn1"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  },
  "theme": {
    "variable": "blue",
    "keyword": "purple",
    "type": "green",
    "identifier": "blue",
    "number": "red"
  }
}